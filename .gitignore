# use glob syntax
syntax: glob

*.class
*.log
*.swp
.#*
.DS_Store
st-export.sqlite

# ignore Maven/Ant build directories and Eclipse, IDEA, Netbeans project files
target
.classpath
.project
.settings
.idea
*.iml
*.ipr
*.iws
nbactions.xml
st-packager/app
st-packager/deploy

# switch to regexp syntax
syntax: regexp
\.orig$
\.orig\..*$
\.chg\..*$
\.rej$
\.conflict\~$
